/* pages/spliceMenu/spliceMenu.wxss *//**index.wxss**/
page{
  background-color:#ffa000;
}
.page-body-wrapper {
  height: 100%;
  width: 100%;
  background-color:#ffa000;
}
.bg-img{
  position: absolute;
  bottom: 0;
  width: 100%;
  height:100%;
}
.btn-area{
  position: absolute;
  width:80%;
  left:50%;
  top:35%;
  transform: translate(-50%,-50%);
  z-index: 5;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center; 
  
}
.btn{
  border: 1rpx solid #92630d;
  width:140rpx;
  height:140rpx;
  padding: 0 10rpx;
  font-size: 30rpx;
  text-align: center;
  border-radius: 10rpx;
  background-color: #ffd999;
}
.btn:active{
  background-color: #ccc;
}
.icon-longphoto{
  background-image: url("https://www.easyicon.net/download/png/544780/256/");
  background-size: cover;
  display: block;
  width: 90rpx;
  height: 90rpx;
  margin: 0 auto;
}
.icon-frame{
  background-image: url("https://www.easyicon.net/download/png/540132/256/");
  background-size: cover;
  display: block;
  width: 90rpx;
  height: 90rpx;
  margin: 0 auto;
}


